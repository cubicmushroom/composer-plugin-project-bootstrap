{
    "name": "cubicmushroom/composer-plugin-project-bootstrap",
    "description": "Composer plugin to bootstrap projects",
    "type": "composer-plugin",
    "license": "MIT",
    "require": {
        "composer-plugin-api": "^1.0"
    },
    "require-dev": {
        "composer/composer": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "CubicMushroom\\Composer\\Plugin\\": "src/"
        }
    },
    "extra": {
        "class": "CubicMushroom\\Composer\\Plugin\\ProjectBootstrap"
    }
}
